    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.1",
    "@types/react": "17.0.4",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
