    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-eslint": "10.1.0",
    "chalk": "4.1.1",
    "connected-react-router": "6.9.1",
    "cross-env": "7.0.3",
    "dotprop": "1.2.1",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "express-favicon": "2.0.1",
    "fontfaceobserver": "2.1.0",
    "history": "5.0.0",
    "husky": "6.0.0",
    "i18next": "20.2.2",
    "i18next-browser-languagedetector": "6.1.0",
    "i18next-scanner": "3.0.0",
    "inquirer": "8.0.0",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.4",
    "lint-staged": "10.5.4",
    "material-ui-dropzone": "3.5.0",
    "material-ui-image": "3.3.2",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-app": "1.1.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-dropzone": "11.3.2",
    "react-helmet-async": "1.0.9",
    "react-hotkeys-hook": "3.3.1",
    "react-i18next": "11.8.13",
    "react-infinite-scroll-hook": "4.0.1",
    "react-is": "17.0.2",
    "react-markdown": "6.0.1",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "redux": "4.1.0",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "rimraf": "3.0.2",
    "sanitize.css": "12.0.1",
    "serve": "11.3.2",
    "shelljs": "0.8.5",
    "styled-components": "5.2.3",
    "stylelint": "13.13.0",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "web-vitals": "1.1.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "devstart": "react-scripts start",
    "start": "node server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "description": "AI-powered art recommendation app",
  "main": "index.js",
  "repository": "https://github.com/tracycollins/art47.git",
  "author": "tracycollins <tc@threeceemedia.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "redux-devtools-extension": "2.13.9"
  }
}
